<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="EggFertBase">
		<defName>RW_PinkLizardEggFertilized</defName>
		<label>pink lizard egg (fert.)</label>
		<description>A fertilized pink lizard egg. If all goes well, it should hatch into a baby pink lizard. It can be eaten raw, but it's much better cooked.</description>
		<possessionCount>1</possessionCount>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/EggOval</texPath>
			<color>(225, 210, 230)</color>
			<drawSize>1.2</drawSize>
		</graphicData>
		<statBases>
			<Nutrition>0.6</Nutrition>
			<MarketValue>25.0</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>7</hatcherDaystoHatch>
				<hatcherPawn>RW_PinkLizard</hatcherPawn>
			</li>
		</comps>
	</ThingDef>
</Defs>